    "# 17:52\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "dp=[0]*(n+1)\n",
    "dp[0]=1\n",
    "dp[1]=1\n",
    "if n>1:\n",
    "    for i in range(2,n+1):\n",
    "        dp[i]=(dp[i-1]+dp[i-2]*2)%796796\n",
    "\n",
    "print(dp[n])"
   ]
